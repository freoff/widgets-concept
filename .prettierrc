{
  "$schema": "http://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "semi": false,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "**/*.html",
      "options": {
        "parser": "angular",
        "htmlWhitespaceSensitivity": "css"
      }
    }
  ]
}
